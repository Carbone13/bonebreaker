[application]

config/name="Bonebreaker"
run/main_scene="res://scenes/main_scene.tscn"
config/icon="res://assets/icons/icon.png"
config/windows_native_icon="res://assets/icons/icon.ico"

[autoload]

GlobalInput="*res://scripts/inputs/Input.cs"
OnlineMatch="*res://addons/nakama-webrtc/OnlineMatch.gd"
Online="*res://addons/nakama-webrtc/Online.gd"
SyncManager="*res://addons/godot-rollback-netcode/SyncManager.gd"
Nakama="*res://addons/com.heroiclabs.nakama/Nakama.gd"
Physics="*res://scripts/physics/Physics.cs"
Main="*res://scripts/Main.gd"

[debug]

settings/fps/force_fps=60
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false
multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/other_window_args="--auto-connect"
multirun/first_window_args="--auto-connect"

[display]

window/size/width=320
window/size/height=180
window/size/resizable=false
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true
window/vsync/use_vsync=false
mouse_cursor/custom_image="res://assets/cursor.png"
mouse_cursor/custom_image_hotspot=Vector2( 32, 0 )
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/codeandweb.texturepacker/plugin.cfg", "res://addons/godot-rollback-netcode/plugin.cfg", "res://addons/multirun/plugin.cfg" )

[gdnative]

singletons=[ "res://webrtc/webrtc.gdnlib" ]

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}
mp3={
"loop": false,
"loop_offset": 0
}

